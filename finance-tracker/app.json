      "package": "com.anonymous.financetracker"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "0139d556-39cd-40f0-8bbd-7c0fedfdd3c0"
